{
  "name": "dmcnicks-gitlab",
  "version": "1.0.2",
  "author": "dmcnicks",
  "summary": "Installs and configures Omnibus Gitlab and provides custom types for creating projects and users",
  "license": "Apache-2.0",
  "source": "https://github.com/dmcnicks/dmcnicks-gitlab",
  "project_page": "https://github.com/dmcnicks/dmcnicks-gitlab",
  "issues_url": "https://github.com/dmcnicks/dmcnicks-gitlab/issues",
  "dependencies": [
    {"name":"puppetlabs-stdlib","version_requirement":">= 4.5.0"},
    {"name":"camptocamp-openssl","version_requirement":">= 1.2.2"}
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease":[ "7.7" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease":[ "14.04" ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease":[ "6.6", "7.0" ]
    },
    { 
      "operatingsystem": "RedHat",
      "operatingsystemrelease":[ "6.6" ] 
    }
  ]
}
